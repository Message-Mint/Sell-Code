# .env.example

# CORS settings
CORS_ORIGIN=http://your-allowed-origin.com

# Database configuration
DB_HOST=your_database_host       # Database host (e.g., localhost or a specific IP)
DB_USER=your_database_user       # Database username
DB_PASSWORD=your_database_password # Database password
DB_NAME=your_database_name        # Name of the database to connect to
DB_PORT=3306                     # Default MySQL port (change if needed)
DB_CONNECTION_LIMIT=10           # Maximum number of connections in the pool
DB_QUEUE_LIMIT=0                 # Maximum number of queued connection requests
